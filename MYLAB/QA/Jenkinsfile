pipeline {
   agent any
   environment {
        
        BRANCH_NAME = "dev"
   }
   stages{
       stage("first"){
            when { 
               expression { BRANCH_NAME == 'master' }
            }   
            steps{
               script{
                   sh 'echo "QA env"'
                   sh 'uptime'
               }
            }
       }
   }
}

